doctype html
html(style='overflow-y: hidden;')
    head
        title OnlineOS
        meta(charset='utf-8')
        meta(name='viewport',
            content='width=device-width, initial-scale=1, shrink-to-fit=no')
        link(rel='stylesheet',
            href='https://use.fontawesome.com/releases/v5.8.2/css/all.css',
            integrity='sha384-oS3vJWv+0UjzBfQzYUhtDYW+Pj2yciDJxpsK1OYPAYjqT085Qq/1cq5FLXAZQ7Ay',
            crossorigin='anonymous')
        link(rel='stylesheet', href='https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css')
        link(rel='icon' href='/images/favicon.png')
        link(rel='stylesheet', href='/stylesheets/style.css')

    body(style='margin: 0; padding: 0; background: aliceblue;')
        div(class='container-fluid')
            div(class='row')
                div(class='col-5 col-md-4 col-lg-3 filer-menu',
                    style='padding-top: 10px;')
                    details
                        summary(class='home') Bureau
                    details
                        summary(class='documents') Documents
                    details
                        summary(class='images') Images
                    details
                        summary(class='musics') Musiques
                    details
                        summary(class='downloads') Téléchargements
                    details
                        summary(class='videos') Vidéos
                    details
                        summary(class='trash') Corbeille
                div(class='col-7 col-md-8 col-lg-9 filer-body',
                    style='padding-top: 10px;')

        script(src='https://code.jquery.com/jquery-3.3.1.min.js')
        script(src='https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js',
            integrity='sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1'
            crossorigin='anonymous')
        script(src='https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js',
            integrity='sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM',
            crossorigin='anonymous')
        script(src='/javascripts/script-filer.js')
